<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Version>0.0.29</Version>
        <Title>GradsSharp.DataReader.GFS</Title>
        <Authors>Chase2be</Authors>
        <Description>GFS DataReader for GradsSharp</Description>
        <Copyright>Chase2be</Copyright>
        <PackageProjectUrl>https://github.com/weatherportal/GradsSharp</PackageProjectUrl>
        <RepositoryUrl>https://github.com/weatherportal/GradsSharp</RepositoryUrl>
        <RepositoryType>Github</RepositoryType>
        <AssemblyVersion>0.0.29.0</AssemblyVersion>
        <FileVersion>0.0.29.0</FileVersion>
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="NGrib" Version="0.8.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\GradsSharp\GradsSharp.csproj" />
    </ItemGroup>

</Project>
